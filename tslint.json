{
  "defaultSeverity": "error",
  "extends": [
      "tslint:latest",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "prefer-object-spread": false,
    "curly": true,
    "no-var-requires": false,
    "ordered-imports": false,
    "quotemark": [true, "single", "avoid-escape"],
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "rulesDirectory": []
}
